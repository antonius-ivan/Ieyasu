{
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "https://localhost:44348/",
      "sslPort": 44348
    }
  },

  "profiles": {
    "Kestrel": {
      "commandName": "Project",
      "launchBrowser": true,
